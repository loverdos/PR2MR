{
  "manifest_version": 3,
  "name": "GitHub to GitLab Terminology",
  "version": "1.0",
  "description": "Changes GitHub PR terminology to GitLab MR terminology",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage"]
}
